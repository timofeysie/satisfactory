{
  "hosting": {
    "public": "dist/apps/trendy/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "**", "function": "universal" }
    ]
  },
"functions": {
    "predeploy": [
      "npm lint functions",
      "npm build functions --prod"
    ],
    "source": "/"
  }
}
